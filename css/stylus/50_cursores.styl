body
	background-color #FF80C0
	color #FF80C0
	font 16px Arial
section
	border 10px solid #FF3E9E
	background-color #fff
	height 300px
	margin 40px auto
	text-align center
	width 400px
section.pointer
	cursor pointer
section.move
	cursor move
section.wait
	cursor wait
section.help
	cursor help
section.image
	cursor url(../imgs/mario.png), default
	
